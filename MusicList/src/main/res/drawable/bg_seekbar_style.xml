<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 背景 -->
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="#D6D6D6"
                android:centerY="0.75"
                android:endColor="#D6D6D6"
                android:startColor="#D6D6D6" />
        </shape>
    </item>

    <!-- 二级进度 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#804F9433"
                    android:centerY="0.75"
                    android:endColor="#804F9433"
                    android:startColor="#804F9433" />
            </shape>
        </clip>
    </item>

    <!-- 一级进度 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#EEE685"
                    android:centerY="0.75"
                    android:endColor="#EEE685"
                    android:startColor="#EEE685" />
            </shape>
        </clip>
    </item>

</layer-list>